{
  "name": "zinc-search",
  "description": "A web console for Zinc",
  "productName": "Zinc Console",
  "private": true,
  "version": "0.2.6",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts ./src/ ./cypress/ && stylelint **/*.vue",
    "lint-autofix": "eslint --ext .js,.vue,.ts ./src/ ./cypress/ --fix && stylelint **/*.vue --fix",
    "e2evite": "vite e2e",
    "e2e": "./node_modules/cypress/bin/cypress open",
    "e2e-ci": "./node_modules/cypress/bin/cypress run --headless",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "test-once": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@quasar/extras": "^1.14.3",
    "apexcharts": "^3.35.3",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "js-cookie": "^3.0.1",
    "jsoneditor": "^9.9.0",
    "quasar": "^2.7.5",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.2",
    "vue3-apexcharts": "^1.4.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@cypress/vue": "^4.0.0",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@quasar/vite-plugin": "^1.0.10",
    "@types/js-cookie": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitest/ui": "^0.18.1",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.11.3",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "happy-dom": "^6.0.4",
    "prettier": "^2.4.1",
    "sass": "^1.32.0",
    "sass-loader": "^13.0.2",
    "stylelint": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.1",
    "vue-tsc": "^0.38.5"
  }
}
